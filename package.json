{
  "name": "express-mongoose-rest-api",
  "version": "1.1.0",
  "description": "REST API server powered by Express/MongoDB with user authentication via JWT",
  "main": "index.js",
  "repository": "https://github.com/arthurkao/express-mongoose-rest-api.git",
  "author": "Arthur C. Kao <arthurckao@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "express",
    "mongoose",
    "openapi",
    "rest",
    "jwt"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "boom": "^7.2.2",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "helmet": "^3.14.0",
    "joi": "^14.0.4",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.3.8",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.1",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-sorted": "^0.2.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.8",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "start": "nodemon -r dotenv/config index.js",
    "test:unit": "./node_modules/mocha/bin/mocha --file ./test/setup.js --recursive ./test/unit/*",
    "test:integration": "./node_modules/mocha/bin/mocha --file ./test/setup.js ./test/integration/dbSetup.js --recursive ./test/integration/*",
    "test": "npm run test:unit & npm run test:integration",
    "test:coverage": "./node_modules/nyc/bin/nyc.js npm run test"
  }
}
